		bio_color				: '454545',
		name_color				: '1f1f1f',
		links_color				: '2B82AD'
	},
	fonts: {
		typekitEnabled: true,
		font_name: 'bree-1,bree-2',
		font_name_size: 80,
		font_heading: 'bree-1,bree-2',
		font_heading_size: 22,
		font_bio: 'bree-1,bree-2',
		font_bio_size: 14,
